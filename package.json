{
  "name": "re_spla_analysis",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "test": "turbo run test",
    "api:generate": "pnpm --filter @app/backend openapi:generate && pnpm --filter @app/frontend api:generate"
  },
  "devDependencies": {
    "turbo": "^2.1.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0"
  }
}
